<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Nop-Commerce">

        <test  name="Check login with invalid email and valid password ">
            <parameter name="browser" value="edge"/>
            <classes>
                <class name="testcases.TC02_Login">
                    <methods>
                        <include name="loginWithInvalidEmailandValidPassw0rd_N"/>
                    </methods>
                </class>
            </classes>
        </test>
    <test  name="Check login with valid email and invalid password data">
        <parameter name="browser" value="edge"/>
        <classes>
            <class name="testcases.TC02_Login">
                <methods>
                    <include name="loginWithValidEmailandInValidPassw0rd_N"/>
                </methods>
            </class>
        </classes>
    </test>
    <test  name="Check login with invalid email and invalid password ">
        <parameter name="browser" value="edge"/>
        <classes>
            <class name="testcases.TC02_Login">
                <methods>
                    <include name="loginWithInvalidEmailandInValidPassw0rd_N"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>